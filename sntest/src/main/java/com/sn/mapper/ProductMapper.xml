<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sn.mapper.ProductMapper">

	<select id="getList" resultType="com.sn.entity.Product">
		select * from product
	</select>

	<select id="findById" resultType="com.sn.entity.Product">
		select * from product where id=#{userName}
	</select>

</mapper>